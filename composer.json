	"type": "wordpress-theme",
	"minimum-stability": "beta",
	"authors": [
		{
			"name": "Jeremy Mullis",
			"email": "jeremy.mullis@gmail.com",
			"homepage": "https://github.com/drdogbot7"
		}
	],
	"require": {
		"timber/timber": "^2.00",
		"hellonico/timber-dump-extension": "^1.0",
